#!/bin/sh


# Load common.igni with common tasks:
. /Shared/Igniters/common.igni


# Initialize service:
ignite \
    name="Postgresql13" \
    action="${1:-status}" \
    start_cmd="'pg_ctl -D /Services/Postgresql13/data -l /Services/Postgresql13/service.log start'" \
    stop_cmd="'pg_ctl -D /Services/Postgresql13/data -l /Services/Postgresql13/service.log stop'"
